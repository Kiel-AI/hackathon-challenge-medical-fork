  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/body-parser": "^1.16.8",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/ioredis": "^4.16.2",
    "@types/lodash": "^4.14.91",
